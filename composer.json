{
    "name": "ready2study/app",
    "description": "Ready2Study - Transform PDFs into Study Material",
    "type": "project",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "smalot/pdfparser": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}


